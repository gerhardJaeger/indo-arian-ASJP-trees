@article{Labov1981,
	Annote = {Addresses the Neogrammarian controversy (framed by L: ``In the evolution of sound systems, is the basic unit of change the word or the sound?'') particularly in the light of studies which have given credence to lexical diffusion. Sets out to observe, via sound changes in progress, what the basic unit of change is. Observes /ae/ tensing in the English of New York and Philadelphia, as well as vowel fronting in northern inland cities. Finds irregularity in the former (``redistribution of an abstract word class into other abstract classes'') and regularity in the latter (``low-level output rules''), but admits that his study does not identify ``the full range of properties that will allow us to solve the transition problem.''},
	Author = {William Labov},
	Date-Added = {2018-11-26 12:58:45 +0000},
	Date-Modified = {2018-11-26 12:58:45 +0000},
	Journal = {Language},
	Pages = {267-308},
	Title = {Resolving the {N}eogrammarian controversy},
	Volume = {57},
	Year = {1981}}

@article{Labov2007,
	Annote = {An investigation into sound changes and their actuation and spread. Here, L makes a distinction between ``transmission'' of sound changes between generations and their ``diffusion'' from one community to another, the latter of which causes ``weakening of the original pattern'' and ``loss of structural features.'' As with L 1981, L focuses here on /ae/ tensing and the Northern Cities shift. The former phenomenon is shown to lose many of its original conditioning factors as it diffuses from New York to neighboring cities. Additionally, the Northern Cities shift is shown to be incompletely implemented among St. Louisans with a great deal of overlap between categories, pointing to the borrowing of individual sound changes rather than the entire pattern---this is interesting, since in L 1981, the NCS exemplified a low-level output rule defined by fairly regular sound change. L additionally Draws some interesting connections between sound change and political and social upheaval.},
	Author = {William Labov},
	Date-Added = {2018-11-26 12:58:45 +0000},
	Date-Modified = {2018-11-26 12:58:45 +0000},
	Journal = {Language},
	Pages = {344--387},
	Title = {Transmission and Diffusion},

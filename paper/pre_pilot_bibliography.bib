@inproceedings{Olsen2005,
	Address = {Wiesbaden},
	Annote = {This paper proceeds from a discussion of the declension of Iranian loans in Armenian. In most cases, the stem type of the Armenian borrowing corresponds to that of the Iranian word. However, in some cases, there are discrepancies between the final vowel of the Iranian word and the Armenian word. In some cases, Armenian inflects Iranian loans as \textit{-o}-stems, but there are no \textit{-o}-stems in Iranian. The author suggests that the somewhat random correspondence between Iranian \textit{-a}-stems and Armenian \textit{-a}, \textit{-o} and \textit{-u}-stems has to do with the dialectal diversity of Iranian sources. Parthian and Middle Persian are generally the most identifiable source languages of Iranian loans in Armenian, but O suggests that loanwords from East Iranian languages may be present in Armenian as well, invoking the ``Parnian'' East Iranian element in Parthian.},

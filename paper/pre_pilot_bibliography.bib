@book{Bloomfield1933,
	Address = {New York},
	Annote = {The chapter ``Dialect Geography'' follows ``The Comparative Method,'' appropriately so, since it discusses apparent exceptions to the notion of regularity in sound change.

B appeals to the ``principle of density'' to explain dialectal differentiation, hypothesizing that a speakers adapt the speech-habits of their interlocutors, either giving up old forms, adopting new ones, or changing the frequency of forms without abandoning old ones or adopting ``really'' new ones. B suggests that the spread of an innovation is limited when it comes up against weaknesses in the network of oral communication, which can be caused by topographical or social boundaries.

B associates various European isoglosses with extralinguistic sociopolitical or religious boundaries. B also discusses overlapping isoglosses, most notably the fact that the vowels in the Dutch words for `house' and `mouse' do not align with other linguistic features, and do not satisfactorily define a linguistic area.},
	Author = {Leonard Bloomfield},
	Date-Added = {2018-11-26 12:58:45 +0000},

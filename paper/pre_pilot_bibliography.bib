@unpublished{Kuemmel2011,
	Annote = {This paper analyzes Indo-Iranian languages according to whether they show a tendency toward ``word-related'' or ``syllable-related'' developments, according to the typology of Auer 1993. Traits seen in Indic, such as cluster simplification and external sandhi, are traits of a syllable-related language, whereas epenthesis at borders is more typical of a word-related language. K notes that Middle East Iranian languages are almost entirely word-related in their treatment of phonological phenomena, but identifies a trend toward syllable-related developments in West Iranian, especially Persian.},
	Author = {Martin K\"ummel},

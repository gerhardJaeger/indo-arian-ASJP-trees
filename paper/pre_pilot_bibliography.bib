@inproceedings{Mallory2002,
	Address = {Oxford},
	Annote = {This paper presents five archeological models and their relation to historical linguistics: (1) the continuity model, the premise of which is that if there has been no major lacuna in the archeological record, there has been linguistic continuity (this model is employed in the Indigenous Aryans Theory); (2) the discontinuity model, which argues that where there are apparent breaks in an archeological record, there has been linguistic intrusion; (3) the geographical model, where locations are sought in correlation with a language's distribution; (4) the cultural model, where material culture is associated with a particular language; and (5) the contact model, where language spread is thought to be due to language shift. Indo-Iranian archaeological records are geographically and chronologically disparate. The four zones corresponding to Indo-Iranian migration are sequenced as follows: the Dnieper-Ural region serves as the staging ground for the development of Proto-Indo-Iranian; the language moves to the Ural-Yenisei region, and then to the Bactria-Margiana Archeological Complex, finally dispersing into Iran and India. M introduces the ``Kulturkugel'' model, which proposes that it is possible for a group to maintain its social organization and language but adopt a new material culture, which describes the linguistic, social and cultural situation of the early Indo-Iranians following their passage through BMAC.},
	Author = {James P. Mallory},
	Booktitle = {Indo-Iranian Languages and Peoples},
	Date-Added = {2018-11-26 12:58:45 +0000},
	Date-Modified = {2018-11-26 12:58:45 +0000},
	Editor = {Nicholas Sims-Williams},
	Pages = {19-42},
	Publisher = {Oxford University Press},
	Series = {Proceedings of the British Academy},

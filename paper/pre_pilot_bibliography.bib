@article{Gershevitch1962,
	Annote = {This paper reassesses Old Persian words traditionally ascribed to ``Median'' admixture, viz., words with \textit{s}, \textit{z} (as against \textit{\texttheta}, \textit{d}) $<$ PIr *\'{s}, *\'{z}; words showing \textit{$\vartheta$r} instead of OP \textit{\c{c}} $<$ PIr *{\texttheta}r; and words showing \textit{sp} instead of \textit{s} $<$ PIr *\'sw. Gershevitch begins with a discussion of words in Middle and New Persian showing variation between \textit{\v{s}} and \textit{h} for PIr *\'{s}y, e.g., NP \textit{mih} `greater' : Av \textit{masiia-}; NP \textit{be\v{s}} `more' $<$ *wa\'sya-.


@article{Szmrecsanyi2011,
	Annote = {Argues for corpus-based dialectometry over single-feature atlas-based dialectology. The advantages of using a corpus is that observations can be frequency-based rather than hinging on an oversimplified, reduced data. The categorical, discrete nature of isoglosses is an oversimplification found in atlases as well. S's methodological sketch involves counting the frequencies of various morphosyntactic features found in a natural speech corpus of English. He then calculates Euclidean distances between the frequencies of features in different dialects; the distribution of these distances can be compared with the distribution of geographic distances between each dialect. S points out some interesting dissimilarities between relatively closely-situated English dialects in Great Britain. S then converts his measures of similarity and distance into a continuum map; using multi-dimensional scaling, he is able to assign a number of polygons representing dialects a hue according to its position in three-dimensional space. The hues of continuous polygons are not often gradient, signifying that geographic continuity and dialectal continuity do not always go hand in hand. Finally, S uses a clustering algorithm which initially constructs a dendrogram based on cophenetic distances between different data points.

S stresses the fact that this is a methodological sketch that has not paid much attention to the broader linguistic implications of the dialect differences measured. He mentions also that this sort of analysis is not limited to morphosyntactic phenomena, but that ``phonology, lexis and even pragmatics are all in principle amenable to dialectometrical analysis using a corpus-based approach.''

A benefit of the dialect atlas is that it is often interpretable from a standpoint of diachronic linguistics; isoglosses often correspond to diachronic developments. This sort of dialect study seems to potentially deviate from that sort of diachronic applicability. And I wonder about the clarity of the linguistic signal based on aggregate morphosyntactic data. Focusing on different frequencies/rates of one particular morphosyntactic feature (controlling, of course, for individual variation, etc.) across different dialects could be potentially sociolinguistically informative. However, it is not necessarily clear how to interpret aggregate, corpus-based data.},
	Author = {Benedikt Szmrecsanyi},
	Date-Added = {2018-11-26 12:58:45 +0000},
	Date-Modified = {2018-11-26 12:58:45 +0000},
	Journal = {Corpora},
	Number = {1},
	Pages = {45-76},
	Title = {Corpus-based dialectometry: a methodological sketch},
	Volume = {6},
	Year = {2011}}

@incollection{Szmrecsanyi2012,
	Address = {Berlin},
	Annote = {The objective of this paper is to test the ``Fundamental Dialectology Principle,'' the assumption that geographic similarity predicts dialectal similarity (or as a corrolary, that geographic distance predicts dialectal difference). Measures aggregate morphosyntactic Euclidean distance against various measures of geographic distance, and fails to find a linear relationship, and finds very little of the variance explained. Using Trudgill's gravity model, which combines geographic data with population statistics, about 24% of the variance is explained. S finds a significant correlation between aggregate morphosyntactic distance and cophenetic distance (Ward's method)---a clustering algorithm used by S to join his dialectal data points into discrete dialect groups. (If he has measured the Euclidean distances and the cophenetic distances according to the same data, should this be a surprise?)

S's conclusion is that geography on its own is not much of a predictor. He mentions the possibility that morphosyntactic variability -- and especially syntactic variability -- is different from phonological or lexical variability. However, he concludes that the most cogent explanation for his result is that ``compared to frequency-based approaches, atlas-based approaches overrate geography,'' due to their categorical nature.},
	Author = {Benedikt Szmrecsanyi},
	Booktitle = {Dialectological and Folk Dialectological Concepts of Space},
	Date-Added = {2018-11-26 12:58:45 +0000},
	Date-Modified = {2018-11-26 12:58:45 +0000},
	Editor = {S. Hansen and C. Schwarz and P. Stoeckle and T. Streck},
	Pages = {215-288},
	Publisher = {Walter de Gruyter},
	Title = {Geography is overrated},
	Year = {2012}}

